{
  "name": "endtoend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate:nexus": "nodemon --exec \"ts-node --transpile-only -P nexus.tsconfig.json pages\\api\\graphql\" --ext \"ts\" --watch \"*\\graphql\\**\\*.ts\"  ",
    "generate:genql": "nodemon --exec \"genql --schema .\\graphql\\schema.graphql --output .\\graphql\\generated\\genql\" --watch \"graphql\\schema.graphql\"  ",
    "generate": "concurrently \"npm run generate:nexus \" \"npm run generate:genql\" "
  },
  "dependencies": {
    "@genql/runtime": "^2.6.0",
    "@prisma/client": "^3.2.0",
    "apollo-server-micro": "2.25",
    "graphql": "^15.6.1",
    "graphql-scalars": "^1.11.0",
    "next": "11.1.2",
    "concurrently": "^6.3.0",
    "nexus": "^1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@genql/cli": "^2.6.0",
    "@types/react": "17.0.27",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "nodemon": "^2.0.13",
    "prisma": "^3.2.0",
    "ts-node": "^10.2.1",
    "typescript": "4.4.3"
  }
}